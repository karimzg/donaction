{
  "kind": "collectionType",
  "collectionName": "connected_accounts",
  "info": {
    "singularName": "connected-account",
    "pluralName": "connected-accounts",
    "displayName": "Connected Account",
    "description": "Stripe Connect account information for klubrs"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "stripe_account_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "klubr": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::klubr.klubr",
      "inversedBy": "connected_account"
    },
    "account_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "active",
        "restricted",
        "disabled"
      ],
      "default": "pending",
      "required": true
    },
    "capabilities": {
      "type": "json"
    },
    "verification_status": {
      "type": "enumeration",
      "enum": [
        "unverified",
        "pending",
        "verified",
        "rejected"
      ],
      "default": "unverified",
      "required": true
    },
    "onboarding_completed": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "business_type": {
      "type": "enumeration",
      "enum": [
        "individual",
        "company",
        "non_profit"
      ]
    },
    "country": {
      "type": "string",
      "default": "FR"
    },
    "created_at_stripe": {
      "type": "datetime"
    },
    "last_sync": {
      "type": "datetime"
    },
    "requirements": {
      "type": "json"
    },
    "charges_enabled": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "payouts_enabled": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "uuid": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid",
      "options": {
        "disable-regenerate": true,
        "disable-auto-fill": true
      }
    }
  }
}
