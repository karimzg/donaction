{
  "kind": "collectionType",
  "collectionName": "webhook_logs",
  "info": {
    "singularName": "webhook-log",
    "pluralName": "webhook-logs",
    "displayName": "Webhook Log",
    "description": "Stripe webhook event logs"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "event_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "event_type": {
      "type": "string",
      "required": true
    },
    "account_id": {
      "type": "string"
    },
    "payload": {
      "type": "json"
    },
    "processed": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "error_message": {
      "type": "text"
    },
    "retry_count": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "processed_at": {
      "type": "datetime"
    },
    "uuid": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid",
      "options": {
        "disable-regenerate": true,
        "disable-auto-fill": true
      }
    }
  }
}
