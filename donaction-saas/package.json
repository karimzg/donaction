{
  "name": "svelte-wc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:build": "vite build --config vite.config.development.ts",
    "dev:watch": "vite build --config vite.config.development.ts --watch",
    "dev:serve": "vite --port 3101",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage2": "vitest run --coverage --coverage-provider=v8",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "build": "BUILD_MODE=INDIVIDUAL vite build"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/google.maps": "^3.58.1",
    "@types/grecaptcha": "^3.0.9",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "jsdom": "^27.4.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "sass": "^1.80.4",
    "svelte": "^5.0.3",
    "svelte-check": "^4.0.5",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.9",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@stripe/stripe-js": "^4.9.0",
    "lottie-web": "^5.12.2",
    "swiper": "^11.2.2"
  }
}
