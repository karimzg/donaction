{
  "kind": "collectionType",
  "collectionName": "klubr_houses",
  "info": {
    "singularName": "klubr-house",
    "pluralName": "klubr-houses",
    "displayName": "Klubr House",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "import-export-entries": {
      "idField": "slug"
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Le Klub",
      "required": true
    },
    "club_presentation": {
      "type": "dynamiczone",
      "components": [
        "club-presentation.mot-du-dirigeant",
        "club-presentation.section-citation",
        "club-presentation.pourquoi-klubr-accompagne",
        "club-presentation.localisation",
        "composant-atoms.section-texte-image"
      ]
    },
    "couvertureMedia": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "klubr": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::klubr.klubr",
      "mappedBy": "klubr_house"
    },
    "primary_color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "secondary_color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "chiffres": {
      "type": "dynamiczone",
      "components": [
        "club-chiffres.chiffres"
      ],
      "min": 1,
      "max": 3
    },
    "description": {
      "type": "blocks",
      "required": false
    },
    "header_text_color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "metaDescription": {
      "type": "text",
      "maxLength": 300
    },
    "uuid": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid",
      "options": {
        "disable-regenerate": true,
        "disable-auto-fill": true
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "partnerList": {
      "type": "dynamiczone",
      "components": [
        "composant-atoms.partner-item"
      ]
    },
    "footer_text_color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "poster_media": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "poster_primary_color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    }
  }
}
